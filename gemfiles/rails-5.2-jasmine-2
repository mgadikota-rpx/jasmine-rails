source 'https://rubygems.org'

gemspec path: File.expand_path('..', File.dirname(__FILE__))

gem 'rails', '~> 5.2.0'
gem 'bootsnap', require: false
gem 'puma'

gem 'coffee-rails'
gem 'sass-rails'

gem 'jquery-rails'
gem 'sqlite3'

gem 'jasmine-core', '~>2.0'

group :development do
  gem 'listen'
end

group :test do
  gem 'rspec-rails'
  gem 'capybara'
  gem 'poltergeist'
  gem 'pry'
end
